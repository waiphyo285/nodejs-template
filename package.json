        "unit:test": "mocha -r module-alias/register './**/*.spec.js' --exit"
    },
    "dependencies": {
        "async": "^3.2.6",
        "bcrypt": "5.1.1",
        "cookie-parser": "^1.4.7",
        "cookie-session": "^2.1.1",
        "cors": "^2.8.5",
        "csrf-csrf": "2.3.0",
        "debug": "2.6.9",
        "dotenv": "8.6.0",
        "express": "4.22.1",
        "express-rate-limit": "6.11.2",
        "express-ws": "^5.0.2",
        "fs-extra": "10.1.0",
        "http-errors": "1.6.3",
        "i18n-node": "^2.1.5",
        "joi": "14.3.1",
        "js-beautify": "^1.15.4",
        "jsonwebtoken": "^9.0.3",
        "module-alias": "^2.2.3",
        "moment": "^2.30.1",
        "mongodb-memory-server": "8.16.1",
        "mongoose": "5.13.23",
        "morgan": "1.9.1",
        "multer": "1.4.4",
        "mysql2": "3.15.3",
        "passport": "0.4.1",
        "passport-local": "^1.0.0",
        "pluralize": "^8.0.0",
        "pug": "^3.0.3",
        "redis": "2.8.0",
        "sequelize": "^6.37.7",
        "socket.io": "^4.8.1",
