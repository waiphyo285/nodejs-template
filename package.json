{
    "name": "sync-template-backend",
    "description": "Sync Template Backend Project",
    "author": "Wai Phyo (waiphyo.dev@gmail.com)",
    "version": "1.0.0",
    "private": true,
    "keywords": [
        "itemplate",
        "backend"
    ],
    "scripts": {
        "format:check": "prettier --check .",
        "format:write": "prettier --write .",
        "start": "node -r module-alias/register ./bin/www",
