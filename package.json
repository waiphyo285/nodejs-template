    "devDependencies": {
        "chai": "4.5.0",
        "chai-http": "4.4.0",
        "eslint": "8.57.1",
        "eslint-config-prettier": "8.10.2",
        "mocha": "9.2.2",
        "prettier": "2.8.8"
    },
    "_moduleAliases": {
        "@bin": "bin",
        "@config": "config",
        "@routes": "routes",
        "@resources": "resources",
        "@utils": "utils",
